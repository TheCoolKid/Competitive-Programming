#include <iostream>
using namespace std;

int main() {
	int n;
	scanf("%d",&n);
	int count1 = 0;
	int count2 = 0;
	for(int i = 0; i < n; i++){
		int a,b;
		scanf("%d%d",&a,&b);
		if(a > b){
			count1++;
		}
		if(a< b){
			count2++;
		}
	}
	if(count1 > count2){
		printf("Mishka");
	}
	else if(count1 < count2){
		printf("Chris");
	}
	else
		printf("Friendship is magic!^^");
	return 0;
}
